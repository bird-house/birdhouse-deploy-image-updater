{
    "version": 1,
    "name": "DACCS E2E docker image updater config",

    "project": {
        "name": "birdhouse-deploy",
        "url": "https://github.com/bird-house/birdhouse-deploy",
        "bump_file": "birdhouse/default.env"
    },

    "images": [
        {
            "name": "finch",
            "url": "https://hub.docker.com/r/birdhouse/finch",
            "dockerhub_repo_name": "birdhouse/finch",
            "bump_tag": "FINCH_IMAGE",
            "tag_filter": ""
        },
        {
            "name": "weaver",
            "url": "https://hub.docker.com/r/pavics/weaver",
            "dockerhub_repo_name": "pavics/weaver",
            "bump_tag": "WEAVER_IMAGE",
            "tag_filter": "manager"
        }
    ]
}